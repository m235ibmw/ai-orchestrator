{
  "preferences": {
    "menuBarEnabled": false,
    "quickEntryShortcut": "off"
  },
  "mcpServers": {
    "ai-orchestrator": {
      "type": "process",
      "command": "node",
      "args": ["{{PROJECT_ROOT}}/mcp-server/dist/index.js"],
      "cwd": "{{PROJECT_ROOT}}/mcp-server"
    },
    "gsheets": {
      "command": "npx",
      "args": ["-y", "mcp-gsheets"],
      "env": {
        "GOOGLE_PRIVATE_KEY": "{{GOOGLE_PRIVATE_KEY}}",
        "GOOGLE_CLIENT_EMAIL": "{{GOOGLE_CLIENT_EMAIL}}"
      }
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@zengwenliang/mcp-server-sequential-thinking"]
    }
  }
}
